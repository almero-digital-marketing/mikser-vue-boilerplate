    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.15.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
