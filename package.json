{
  "name": "mikser-vue-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build && mikser --no-watch --no-server --whitebox-global && grunt",
    "deploy": "vue-cli-service build && mikser --no-watch --no-server --whitebox-global && grunt deploy",
    "dev": "concurrently \"mikser --no-server\" \"vue-cli-service serve\"",
    "edit": "concurrently \"vue-cli-service build --mode development --watch\" \"mikser\"",
    "publish": "mikser --no-watch --no-server --whitebox-refresh --whitebox-global"
  },
